{
  "name": "@min3studios/klinecharts-pro",
  "version": "0.2.3",
  "description": "Financial chart with advanced order management built on KLineChart - TradingView/Bybit style order placement and management.",
  "type": "module",
  "main": "./dist/klinecharts-pro.umd.js",
  "module": "./dist/klinecharts-pro.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/klinecharts-pro.js",
      "require": "./dist/klinecharts-pro.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "CUSTOMKLINEAPI.md",
    "PUBLISHING.md",
    "SETUP.md",
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/min3studios/klinecharts-pro"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "keywords": [
    "klinecharts",
    "pro",
    "candlestick",
    "finance",
    "stock",
    "chart",
    "canvas",
    "trading",
    "orders",
    "order-management",
    "tradingview",
    "bybit",
    "pnl",
    "stop-loss",
    "take-profit",
    "limit-orders"
  ],
  "author": "liihuu",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "build-core": "tsc && vite build",
    "build-dts": "dts-bundle-generator --no-banner true --umd-module-name klinechartspro -o dist/index.d.ts src/index.ts",
    "build": "pnpm run build-core && pnpm run build-dts && pnpm run verify-build",
    "verify-build": "node scripts/verify-build.js",
    "prepublishOnly": "pnpm run build",
    "publish:patch": "pnpm version patch && pnpm publish",
    "publish:minor": "pnpm version minor && pnpm publish",
    "publish:major": "pnpm version major && pnpm publish",
    "release:patch": "pnpm version patch && pnpm publish && git push --follow-tags",
    "release:minor": "pnpm version minor && pnpm publish && git push --follow-tags",
    "release:major": "pnpm version major && pnpm publish && git push --follow-tags",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:deploy": "pnpm run docs:build && gh-pages -d website"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "20",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "babel-preset-solid": "^1.6.10",
    "dts-bundle-generator": "^7.2.0",
    "eslint": "^8.35.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "gh-pages": "^5.0.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "typescript": "^5.9.2",
    "vite": "^4.1.1",
    "vite-plugin-solid": "^2.6.1",
    "vitepress": "^1.0.0-alpha.75",
    "vitest": "^0.28.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "solid-js": "^1.8.0"
  },
  "peerDependencies": {
    "klinecharts": ">=9.0.0"
  }
}
